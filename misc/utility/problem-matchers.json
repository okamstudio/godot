{
	"problemMatcher": [
		{
			"owner": "gcc",
			"pattern": [
				{
					"regexp": "^(?:\\s+\\d+\\>)?(\\S.*?)[\\(:](\\d+)[,:]?(\\d*)(?::{\\d+:\\d+\\-\\d+:\\d+})?\\)?:\\s+(?:fatal\\s+)?(error|warning|info):\\s+(.*?)(?:\\s*\\[(.*)\\])?$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5,
					"code": 6
				}
			]
		},
		{
			"owner": "msvc",
			"pattern": [
				{
					"regexp": "^(?:\\s+\\d+\\>)?(\\S.*)\\((\\d+),?(\\d*)?(?:,\\d+,\\d+)?\\)\\s*:\\s+(?:fatal\\s+)?(error|warning|info)\\s+(\\w{1,2}\\d+)\\s*:\\s*(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"code": 5,
					"message": 6
				}
			]
		},
		{
			"owner": "godot",
			"pattern": [
				{
					"regexp": "^(?:\\x1b\\[[;\\d]+m)?(?:(?:SCRIPT|SHADER|UNKNOWN)\\s)?(ERROR|WARNING):(?:\\x1b\\[[;\\d]+m)?\\s(.*)$",
					"severity": 1,
					"message": 2
				},
				{
					"regexp": "^(?:\\x1b\\[[;\\d]+m)?\\s+at:.*\\((.*):(\\d+)\\)(?:\\x1b\\[[;\\d]+m)?$",
					"file": 1,
					"line": 2
				}
			]
		}
	]
}
